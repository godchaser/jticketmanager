DROP TABLE "tickets";
CREATE TABLE tickets (ID INTEGER PRIMARY KEY AUTOINCREMENT, ticketCategory text, ticketStatus text, ticketPriority text, ticketSubject text, ticketAssignee text, ticketUpdate text, ticketAuthor text, version INTEGER DEFAULT 0 NOT NULL);
